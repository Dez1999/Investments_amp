*This Calculates at END of EACH COMPOUNDING Period\r\n        if (rateNum === \"\"){       //Check interest rate value\r\n            setRateNum(0.00);\r\n        }\r\n        if (yearNum === \"\"){       //Check year value\r\n            setYearNum(0);\r\n        }\r\n        if (startNum === \"\"){       //Check start value\r\n            setStartNum(0.0);\r\n        }\r\n        var y = parseFloat(startNum) * (Math.pow(parseFloat(rateNum/100) + 1.00, parseFloat(yearNum)));    //Calculate Interest on principle amount by n years; Formula: F = P*(F/P, i, n)\r\n        var d = parseFloat(annualNum) * ((Math.pow(1.00 + parseFloat(rateNum/100), yearNum) - 1) / parseFloat(rateNum/100))     //Calculate total amount on annual uniform payments; Formula: F = A*(((1 +i)^2 - 1)/i)\r\n        var total2 = y + d;\r\n        var fixedTotal = total2.toFixed(2);\r\n        return fixedTotal  /*ADD FEATURE: ADD COMMAS TO ANSWER*/\r\n    }\r\n    \r\n  }\r\n\r\n  const handleSubmit = e => {\r\n    e.preventDefault();\r\n    console.log('submitted!');\r\n    console.log(state);\r\n    /*setTotal(yearNum + rateNum)*/\r\n    setError('');\r\n    console.log(\"Succeeded!!!\")\r\n\r\n    setTotal(calculateResult)\r\n\r\n    /*\r\n    for (let key in state) {\r\n      if (state[key] === '') {\r\n        setError(`You must provide the ${key}`)\r\n        return\r\n      }\r\n    }\r\n    */\r\n   \r\n    // const regex = /^\\w+([-+.']\\w+)*@\\w+([-.]\\w+)*\\.\\w+([-.]\\w+)*$/;\r\n    // const test = regex.test(state.email);\r\n    // console.log(test);\r\n\r\n    \r\n  };\r\n\r\n  const handleInput = e => {\r\n    const inputName = e.currentTarget.name;\r\n    const value = e.currentTarget.value;\r\n\r\n    setState(prev => ({ ...prev, [inputName]: value }));\r\n  };\r\n\r\n  const handlenum1 = (event) => {\r\n       this.setState({\r\n         startAmount: event.target.value\r\n       })\r\n  }\r\n\r\n  const handlenum2 = (event) => {\r\n    this.setState({\r\n      rate: event.target.value\r\n    })\r\n  }\r\n\r\n  const handlenum3 = (event) => {\r\n  this.setState({\r\n    year: event.target.value\r\n  })\r\n  }\r\n\r\n  const execute =(event) =>{\r\n    this.setState({result: this.state.year + this.state.rate});\r\n    event.prevent.default();\r\n    console.log(\"Succeeded!!!\")\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <GlobalStyle />\r\n      <StyledFormWrapper>\r\n        <StyledForm onSubmit={handleSubmit}>\r\n          <h2>Interest Calculator</h2>\r\n\r\n          <label htmlFor=\"startAmount\">Starting Amount $</label>\r\n          <StyledInput\r\n            type=\"number\"\r\n            name=\"startAmount\"\r\n            value={startNum}\r\n            onChange={e => setStartNum(+e.target.value)}\r\n          />\r\n\r\n          <label htmlFor=\"rate\">Interest Rate</label>\r\n          <StyledInput\r\n            type=\"number\"\r\n            name=\"rate\"\r\n            value={rateNum}\r\n            onChange={e => setRateNum(+e.target.value)}\r\n          />\r\n\r\n          <label htmlFor=\"year\">Number of Years</label>\r\n          <StyledInput\r\n            type=\"number\"\r\n            name=\"year\"\r\n            value={yearNum}\r\n            onChange={e => setYearNum(+e.target.value)}\r\n          />\r\n\r\n          <label htmlFor=\"year\">Annual Payments $</label>\r\n          <StyledInput\r\n            type=\"number\"\r\n            name=\"annual\"\r\n            value={annualNum}\r\n            onChange={e => setAnnualNum(+e.target.value)}\r\n          />\r\n\r\n          <StyledFieldset>\r\n            <legend>Compunding Period</legend>\r\n            <label>\r\n              <input\r\n                type=\"radio\"\r\n                value=\"female\"\r\n                name=\"gender\"\r\n                checked={state.gender === 'female'}\r\n                onChange={handleInput}\r\n              />\r\n              Beginning\r\n            </label>\r\n            <label>\r\n              <input\r\n                type=\"radio\"\r\n                value=\"male\"\r\n                name=\"gender\"\r\n                checked={state.gender === 'male'}\r\n                onChange={handleInput}\r\n              />\r\n              End\r\n            </label>\r\n        \r\n          </StyledFieldset>\r\n          \r\n          {error && (\r\n            <StyledError>\r\n              <p>{error}</p>\r\n            </StyledError>\r\n          )}\r\n          <StyledButton type=\"submit\">Calculate</StyledButton>\r\n          <p><strong> Result: $ </strong> {total}</p>\r\n        </StyledForm>\r\n      </StyledFormWrapper>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default FormBox;","C:\\Users\\Blake\\Documents\\React\\ReactProject1\\invest-app\\src\\Components\\NewsFeed.js",["37"],{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","severity":1,"message":"43","line":2,"column":8,"nodeType":"44","messageId":"45","endLine":2,"endColumn":16},{"ruleId":"42","severity":1,"message":"46","line":52,"column":7,"nodeType":"44","messageId":"45","endLine":52,"endColumn":21},{"ruleId":"42","severity":1,"message":"47","line":179,"column":9,"nodeType":"44","messageId":"45","endLine":179,"endColumn":19},{"ruleId":"42","severity":1,"message":"48","line":185,"column":9,"nodeType":"44","messageId":"45","endLine":185,"endColumn":19},{"ruleId":"42","severity":1,"message":"49","line":191,"column":9,"nodeType":"44","messageId":"45","endLine":191,"endColumn":19},{"ruleId":"42","severity":1,"message":"50","line":197,"column":9,"nodeType":"44","messageId":"45","endLine":197,"endColumn":16},{"ruleId":"42","severity":1,"message":"51","line":1,"column":17,"nodeType":"44","messageId":"45","endLine":1,"endColumn":26},"no-native-reassign",["52"],"no-negated-in-lhs",["53"],"no-unused-vars","'logomain' is defined but never used.","Identifier","unusedVar","'StyledTextArea' is assigned a value but never used.","'handlenum1' is assigned a value but never used.","'handlenum2' is assigned a va